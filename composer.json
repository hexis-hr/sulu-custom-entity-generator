{
    "name": "hexis-hr/sulu-custom-entity-generator-bundle",
    "description": "Hexis Symfony bundle that scaffolds Sulu custom entity CRUD for the Sulu admin via a reusable console command.",
    "keywords": [
        "sulu",
        "sulu custom entity",
        "custom entity generator",
        "symfony bundle"
    ],
    "homepage": "https://github.com/hexis-hr/sulu-custom-entity-generator-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hexis\\SuluCustomEntityGeneratorBundle\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.3 || ^7.0",
        "symfony/dependency-injection": "^6.3 || ^7.0",
        "symfony/http-kernel": "^6.3 || ^7.0",
        "symfony/filesystem": "^6.3 || ^7.0",
        "symfony/config": "^6.3 || ^7.0",
        "doctrine/inflector": "^2.0"
    },
    "extra": {
        "symfony": {
            "bundle": "Hexis\\\\SuluCustomEntityGeneratorBundle\\\\SuluCustomEntityGeneratorBundle"
        }
    }
}
